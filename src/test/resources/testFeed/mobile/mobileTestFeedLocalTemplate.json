{
  "targets": [
    {
      "app": "<appname.apk>",
      "platform": "android",
      "desiredCapabilities": {
        "newCommandTimeout": 300,
        "autoGrantPermissions": true,
        "noReset": true,
        "avdLaunchTimeout": 300,
        "autoAcceptAlerts": true,
        "useKeystore": false
      }
    },
    {
      "app": "<appname.ipa>/<appname.app>",
      "platform": "iOS",
      "desiredCapabilities": {
        "newCommandTimeout": 300,
        "autoGrantPermissions": true,
        "noReset": true,
        "avdLaunchTimeout": 300,
        "autoAcceptAlerts": true,
        "useKeystore": false
      }
    }
  ],
  "serverArguments": [
    "--log-level info",
    "--enable-console-logs"
  ],
  "executables": {
    "appiumJSPath": "",
    "nodePath": ""
  },
  "deviceFilters": {
    "platformVersion": {
      "include": [
      ],
      "operator": "",
      "exclude": []
    },
    "model": {
      "include": [],
      "exclude": []
    },
    "udid": {
      "include": [],
      "exclude": []
    }
  }
}